<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.hfad.tabdemo.ContactListFragment">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:id="@+id/emulator1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <ImageView
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:id="@+id/emu1imageView"
            android:src="@mipmap/ic_launcher"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="EMULATOR 1"
            android:textSize="30dp"
            android:textStyle="bold"
            android:id="@+id/emu1textView"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/emu1imageView"
            android:layout_toEndOf="@+id/emu1imageView" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="5554"
            android:id="@+id/emu1textView2"
            android:layout_below="@+id/emu1textView"
            android:layout_toRightOf="@+id/emu1imageView"
            android:layout_toEndOf="@+id/emu1imageView"
            android:textSize="20dp" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/transparent"
            android:text=">"
            android:id="@+id/button"
            android:layout_alignBottom="@+id/emu1textView2"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/emulator2"
        android:layout_below="@+id/emulator1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <ImageView
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:id="@+id/emu2imageView"
            android:src="@mipmap/ic_launcher"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="EMULATOR 2"
            android:textSize="30dp"
            android:textStyle="bold"
            android:id="@+id/emu2textView"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/emu2imageView"
            android:layout_toEndOf="@+id/emu2imageView" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="5556"
            android:id="@+id/emu2textView2"
            android:layout_below="@+id/emu2textView"
            android:layout_toRightOf="@+id/emu2imageView"
            android:layout_toEndOf="@+id/emu2imageView"
            android:textSize="20dp" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=">"
            android:id="@+id/button2"
            android:background="@color/transparent"
            android:layout_alignBottom="@+id/emu2textView2"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/emulator3"
        android:layout_below="@+id/emulator2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <ImageView
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:id="@+id/emu3imageView"
            android:src="@mipmap/ic_launcher"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="EMULATOR 3"
            android:textSize="30dp"
            android:textStyle="bold"
            android:id="@+id/emu3textView"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/emu3imageView"
            android:layout_toEndOf="@+id/emu3imageView" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="5558"
            android:id="@+id/emu3textView2"
            android:layout_below="@+id/emu3textView"
            android:layout_toRightOf="@+id/emu3imageView"
            android:layout_toEndOf="@+id/emu3imageView"
            android:textSize="20dp" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/transparent"
            android:text=">"
            android:id="@+id/button3"
            android:layout_alignBottom="@+id/emu3textView2"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

    </RelativeLayout>

</RelativeLayout>
